{
  "name": "@jeromefitz/dotfiles",
  "description": "Jerome Fitzgeraldâ€™s dotfiles",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/JeromeFitz/dotfiles"
  },
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/JeromeFitz/dotfiles/issues"
  },
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "keywords": [
    "dotfiles"
  ],
  "scripts": {
    "bootstrap": "sh ./scripts/bootstrap",
    "build": "yarn run bootstrap",
    "clean": "sh ./scripts/clean",
    "clean:install": "yarn run clean && yarn run uninstall && yarn install && yarn run bootstrap",
    "uninstall": "sh ./scripts/uninstall",
    "----": "",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "-----": "",
    "branch": "git-cz -m branch --allow-empty",
    "semantic-release": "semantic-release",
    "------": "",
    "lint:eslint": "eslint . --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "lint:prettier": "prettier \"**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "lint": "yarn lint:prettier --check && yarn lint:eslint",
    "pretty-quick": "pretty-quick"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@jeromefitz/codestyle": "8.0.25",
    "@jeromefitz/conventional-gitmoji": "3.0.3",
    "@jeromefitz/git-cz": "14.2.4",
    "@jeromefitz/semantic": "9.0.4",
    "@jeromefitz/release-notes-generator": "1.0.5",
    "dotenv": "16.0.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "pinst": "3.0.0",
    "rimraf": "3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {}
}
