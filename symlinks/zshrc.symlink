# ğŸ°ï¸ shortcut to this dotfiles path is $ZSH
export ZSH=$HOME/.dotfiles
export BREW_PREFIX=/usr/local
# âš›ï¸ `c [tab]` project folder (move to private?)
export PROJECTS=~/Code

DEFAULT_USER=`whoami`

# ğŸ¥ªï¸ load sandboxd first
source $ZSH/zsh/plugins/sandboxd.plugin.zsh

# âš—ï¸ debugging
# zmodload zsh/zprof

# ğŸ”’ï¸ private `./dotfiles-private` repo stuff
if [[ -a ~/.zshrc.private ]]
then
  source ~/.zshrc.private
fi

# ğŸ› ï¸ config.setup.zsh
if [[ -a $ZSH/zsh/config.setup.zsh ]]
then
  source $ZSH/zsh/config.setup.zsh
fi

# ğŸ“œï¸ Better history
# Credits to https://coderwall.com/p/jpj_6q/zsh-better-history-searching-with-arrow-keys
autoload -U up-line-or-beginning-search
autoload -U down-line-or-beginning-search
zle -N up-line-or-beginning-search
zle -N down-line-or-beginning-search
bindkey "^[[A" up-line-or-beginning-search # Up
bindkey "^[[B" down-line-or-beginning-search # Down

# âš—ï¸ debugging
# zprof

# pnpm
export PNPM_HOME="/Users/jerome/Library/pnpm"
export PATH="$PNPM_HOME:$PATH"
# pnpm end